{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Data Source=localhost\\SQLEXPRESS;User ID=sa;Password=1;Initial Catalog=NavqurtServerV2DB;TrustServerCertificate=True;",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },

  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DatabaseConnection": "Data Source=localhost\\SQLEXPRESS;User ID=sa;Password=1;Initial Catalog=NavqurtServerV2DB;TrustServerCertificate=True;"
  },

  "Jwt": {
    "Issuer": "http://www.uz",
    "Audience": "wwwww",
    "SecurityKey": "23f926fb-dcd2-49f4-0fe2-992aac18f08f",
    "Lifetime": 24
  }
}
